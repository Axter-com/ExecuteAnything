CREATE VIEW v_CompilerProfile
AS 
select Ext, s.PrgLangName, s.ProfileName, 'compile' ExecType
from &quot;FileExt&quot; x, CompilerProfile s
where x.PrgLangName like s.PrgLangName;

DROP VIEW IF EXISTS v_ScriptProfile;
CREATE VIEW v_ScriptProfile
AS
select Ext, s.PrgLangName, s.ProfileName, 'script' ExecType
from &quot;FileExt&quot; x, ScriptProfile s
where x.PrgLangName like s.PrgLangName;
