ALTER TABLE &quot;AdvProfile_Settings&quot; RENAME TO &quot;zzTemp_DeleteMe_AdvProfile_Settings&quot;;
CREATE TABLE &quot;AdvProfile_Settings&quot; (
	&quot;PrgLangName&quot;						TEXT NOT NULL,
	&quot;ProfileName&quot;						TEXT NOT NULL,
	&quot;Notes&quot;								TEXT,
	&quot;PreBuildCmdBin&quot;					TEXT,
	&quot;PreBuildCmdFormat&quot;					TEXT,
	&quot;RebuildCmdBin&quot;						TEXT,
	&quot;RebuildCmdFormat&quot;					TEXT,
	&quot;CleanCmdBin&quot;						TEXT,
